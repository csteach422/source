<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.customplugintest1" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Test">
        <param name="android-package" value="org.csteach.plugin.Test" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="resources/icon/android/ldpi.png" />
    <icon density="mdpi" src="resources/icon/android/mdpi.png" />
    <icon density="hdpi" src="resources/icon/android/hdpi.png" />
    <icon density="xhdpi" src="resources/icon/android/xhdpi.png" />
    <splash density="land-hdpi" src="resources/splash/android/splash-land-hdpi.png" />
    <name>Custom Plugin Test1</name>
    <description>
        a simple test for a custom apache cordova plugin...js only
    </description>
    <author email="ancientlives@gmail.com" href="http://csteach422.github.io">
        ancientlives
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
